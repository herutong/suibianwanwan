<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.lottery.dao.MemberDao">

    <select id="findMemberByAccount" resultType="Member" parameterType="String">
        select member_account,mermber_email,member_password,mermber_name,member_phone,member_leader,member_group,member_num_status,member_see
        from CP_Member
        WHERE member_account = #{memberAccount}
    </select>

</mapper>